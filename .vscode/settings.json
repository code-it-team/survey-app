{
  "cSpell.words": [
    "DTOS",
    "Jalil",
    "fontawesome",
    "parens",
    "readonly"
  ],
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "xml": {
      "attr_quotes": "single"
    }
  }, 
  "files.associations": {
    "*.js": "javascriptreact"
  },
  "cSpell.ignoreWords": [
    "fontawesome",
    "fortawesome",
    "free"
  ],
  "editor.minimap.size": "fit",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  }
}
